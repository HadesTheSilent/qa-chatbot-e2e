{
  "version": 2,
  "builds": [
    {
      "src": "chatbot-sut/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "chatbot-sut/backend/api/message.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/message",
      "dest": "/chatbot-sut/backend/api/message.js"
    },
    {
      "src": "/(.*)",
      "dest": "/chatbot-sut/frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}